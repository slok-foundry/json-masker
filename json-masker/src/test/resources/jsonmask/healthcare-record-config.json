{
  "rules": [
    {
      "match": {
        "type": "regex",
        "value": ".*(ssn|social.*security).*"
      },
      "strategy": {
        "type": "full",
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "regex",
        "value": ".*(policy.*number|subscriber.*id|insurance.*number).*"
      },
      "strategy": {
        "type": "middle",
        "keepLeft": 3,
        "keepRight": 3,
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "regex",
        "value": ".*(card.*number|account.*number|routing.*number).*"
      },
      "strategy": {
        "type": "middle",
        "keepLeft": 4,
        "keepRight": 4,
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "contains",
        "value": "cvv"
      },
      "strategy": {
        "type": "full",
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "contains",
        "value": "email"
      },
      "strategy": {
        "type": "middle",
        "keepLeft": 2,
        "keepRight": 2,
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "regex",
        "value": ".*(phone|mobile).*"
      },
      "strategy": {
        "type": "middle",
        "keepLeft": 3,
        "keepRight": 4,
        "maskChar": "*"
      }
    },
    {
      "match": {
        "type": "regex",
        "value": ".*(birth|dob).*"
      },
      "strategy": {
        "type": "full",
        "maskChar": "*"
      }
    }
  ]
}
